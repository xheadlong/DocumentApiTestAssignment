{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MaxFileSize": "5 MB",
  "AllowedFileTypes": {
    "application/pdf": [ ".pdf" ]
  },
  "BlobStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "DocumentContainerName": "documents",
    "ListContainerName": "document-lists"
  }
}
